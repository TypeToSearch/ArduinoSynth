BoxLayout:
    orientation: 'vertical'
    SampleSlider:
        id: samples
        num_sliders: 16
        size: (400, 100)
    SendButton:
        id: send
        size_hint: (1, 0.25)
        text: 'Send'
        on_press: self.send_pressed()
    Label:
        text: send.status
        size_hint: (1, 0.1)
